{
  "name": "openui5-tour",
  "version": "0.1.0",
  "description": "An OpenUI5 library to give a guided tour through your UI5 webapp.",
  "homepage": "https://github.com/mauriciolauffer/openui5-tour#readme",
  "author": "Mauricio Lauffer",
  "license": "MIT",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5",
    "ui5lab",
    "tour",
    "guide",
    "intro",
    "showcase"
  ],
  "main": "dist/resources/openui5/tour/library-preload.js",
  "files": [
    "dist",
    "src",
    "ui5.yaml"
  ],
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@openui5/ts-types": "^1.114.0",
    "@ui5/middleware-code-coverage": "^1.0.3",
    "@wdio/cli": "^8.24.6",
    "@wdio/devtools-service": "^8.24.6",
    "@wdio/local-runner": "^8.24.6",
    "@wdio/mocha-framework": "^8.24.6",
    "@wdio/spec-reporter": "^8.24.2",
    "eslint": "^8.41.0",
    "eslint-config-mlauffer-ui5": "^0.4.0",
    "eslint-plugin-wdio": "^8.20.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-coveralls": "^2.1.0",
    "karma-ui5": "^3.0.3",
    "wdio-qunit-service": "^0.1.1"
  },
  "scripts": {
    "start": "ui5 serve",
    "start:dist": "ui5 serve --config ui5-dist.yaml",
    "build": "ui5 build --clean-dest",
    "lint": "eslint src test demo",
    "lint:ci": "eslint src test -f @microsoft/eslint-formatter-sarif -o target/eslint.sarif",
    "test": "karma start",
    "test:ci": "karma start karma.ci.conf",
    "xtest": "wdio run ./test/openui5/tour/wdio.conf.mjs --watch",
    "xtest:ci": "wdio run ./test/openui5/tour/wdio.conf.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriciolauffer/openui5-tour.git"
  },
  "bugs": {
    "url": "https://github.com/mauriciolauffer/openui5-tour/issues"
  }
}
