{
  "name": "openui5-tour",
  "version": "0.0.27",
  "description": "An OpenUI5 library to give a guided tour through your UI5 webapp.",
  "homepage": "https://github.com/mauriciolauffer/openui5-tour#readme",
  "author": "Mauricio Lauffer",
  "license": "MIT",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5",
    "ui5lab",
    "tour",
    "guide",
    "intro",
    "showcase"
  ],
  "main": "dist/resources/openui5/tour/library-preload.js",
  "files": [
    "dist",
    "src",
    "ui5.yaml"
  ],
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@openui5/ts-types": "^1.101.0",
    "eslint": "^8.15.0",
    "eslint-config-mlauffer-ui5": "^0.3.2",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-coveralls": "^2.1.0",
    "karma-ui5": "^2.4.0"
  },
  "scripts": {
    "start": "ui5 serve",
    "start:dist": "ui5 serve --config ui5-dist.yaml",
    "build": "ui5 build --clean-dest",
    "lint": "eslint src test demo",
    "lint:fix": "eslint src test demo --fix",
    "lint:ci": "eslint src test -f @microsoft/eslint-formatter-sarif -o target/eslint.sarif",
    "test": "karma start",
    "test:ci": "karma start karma.ci.conf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriciolauffer/openui5-tour.git"
  },
  "bugs": {
    "url": "https://github.com/mauriciolauffer/openui5-tour/issues"
  }
}
